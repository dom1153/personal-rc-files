[core]
	editor = vim
[alias]
	edit = add
	opened = status
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[includeIf "gitdir:/usr/bin"]
	path = ~/.gitconfig-linux
[includeIf "gitdir:/opt/homebrew/bin"]
	path = ~/.gitconfig-mac-m1
[includeIf "gitdir:/usr/local/bin"]
	path = ~/.gitconfig-mac-intel