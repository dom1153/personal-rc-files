# === scripts ===
# recursivly set files and folders to be non-executable
alias fixperms='~/scripts/fixperms.sh'

# === general unix commands ===
# interactive rm instead of no prompt
alias untargz='tar xvf'
alias rm='rm -i'
alias ff="find . -name "
alias dirsize="du -sh"
alias la="ls -la"
alias ll="ls -la"

